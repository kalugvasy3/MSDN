<?xml version="1.0"?>
<configuration>
    <startup> 
        <supportedRuntime version="v4.0" sku=".NETFramework,Version=v4.0"/>
    </startup>

 <appSettings>
   
   <add key="excludeOf" value="SSSSSSSSSS;50022"/>   <!--exclude of SSSSSSSSSS and/or port 50022 from "B" INSERT -  NO SPACE between words ;" -->
   <add key="excludeOfword" value="INSERT;DELETE;DROP;UPDATE;CREATE;ALTER;JOIN"/>  <!--exclude of From Select Screen - Use only simple SELECT () -->
   <add key="rowsInOneTransaction" value="100"/>  <!-- You can change it to 1 (optimum 100....1000) - WILL BE VERY SLOW -->

 </appSettings>

</configuration>
